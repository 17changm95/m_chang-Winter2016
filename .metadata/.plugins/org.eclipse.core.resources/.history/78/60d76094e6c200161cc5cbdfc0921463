public class GoingToTheDogs {
	//main method
	public static void main(String[] args) {
		Dog SallysDog = new Dog("Cookie", 7);
		System.out.println(SallysDog.toString());
		System.out.println(SallysDog.makeNoise());
		
		Dog BobsDog = new Dog("Dalmatian", 55);
		Dog PetesDog = BobsDog;
		System.out.println(PetesDog.makeNoise());
	}
}
